<%= form_with model: @company, local: true do |form| %>

    <div class=" col-md-5"> 
        <%= form.label :name %>
        <%= form.text_field :name,:class => "form-control"  %>
    </div>

    <div class=" col-md-5">
        <%= form.label :web_url %>
        <%= form.text_field :web_url, :class => "form-control"  %>
    </div>

    <br>
    <div class="col-md-5">
        <%= form.submit class: "btn btn-primary"%>
    </div>
    <br>

<% end %>
